
<ul>
<% @events.each do |event| %>
<li class="events" data-latitude="<%=event.lat%>" data-longitude="<%=event.lng%>">


	<%= event.name %> <%= event.event_type %> <%= event.date %>
	
	<%=link_to 'Edit', edit_event_path(:id)%>
	
	<%=link_to 'Show', event_path(:id)%>
	</li>

<% end %>
</ul>

<div id="map"> </div>